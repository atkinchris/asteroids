{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "airbnb",
  "globals": {
    "p5": false
  },
  "rules": {
    "semi": ["error", "never"],
    "object-curly-newline": "off",
    "new-cap": ["error", {
      "newIsCapExceptions": ["p5"]
    }],
    "no-param-reassign": ["error", {
      "props": false
    }],
    "no-console": "off"
  }
}
